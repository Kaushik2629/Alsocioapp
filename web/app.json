{
  "expo": {
    "name": "Alsocio",
    "slug": "alsocio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a237e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Alsocio.AlsocioApp"
    },
    "web": {
      "favicon": "./assets/icon.png",
      "build": {
        "babel": {
          "include": [
            "galio-framework"
          ]
        }
      }
    },
    "android": {
      "package": "com.Alsocio.AlsocioApp"
    },
    "description": "You are in Good Hands!"
  }
}
